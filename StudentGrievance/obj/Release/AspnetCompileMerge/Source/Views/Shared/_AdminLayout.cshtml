@using StudentGrievance.Utilitis;
@{
    Layout = null;
    SessionManager sm = new SessionManager();
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="~/WebAssets/img/favicon.ico">
    <title>MGKVP : @ViewBag.Title</title>
    <!-- Bootstrap Core CSS -->
    <link href="~/Content/VcAssets/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Morries chart CSS -->
    <link href="~/Content/VcAssets/plugins/morrisjs/morris.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="~/Content/VcAssets/css/style.css" rel="stylesheet">
    <link href="~/Content/VcAssets/css/custom.css" rel="stylesheet">
    <!-- You can change the theme colors from here -->
    <link href="~/Content/VcAssets/css/colors/blue.css" id="theme" rel="stylesheet">
    <link href="~/Content/css/jquery-confirm.css" rel="stylesheet" />
    <link href="~/Content/sweetalert/sweetalert.css" rel="stylesheet" />
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    <script src="~/Content/VcAssets/plugins/jquery/jquery.min.js"></script>

    <script src="~/WebAssets/vendor/modernizr/modernizr.min.js"></script>
    <script async src="https://cse.google.com/cse.js?cx=50174859a05733ccb"></script>
    <style>
        .gsc-input {
            background: none !important;
        }
    </style>

</head>

<body class="fix-header fix-sidebar card-no-border">

    <div class="preloader">
        <svg class="circular" viewBox="25 25 50 50">
            <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" />
        </svg>
    </div>

    <div id="main-wrapper">
        <header class="topbar">
            <nav class="navbar top-navbar navbar-expand-md navbar-light">
                <div class="navbar-header">
                    <a class="navbar-brand" href="~/Home/Index" target="_blank">
                        <b> <img src="~/Content/VcAssets/images/logo-icon.png" alt="Logo" class="dark-logo" /> </b> <span>
                            <!--  Mahatma Gandhi Kashi Vidyapith, Varanasi  -->
                            <img src="~/Content/VcAssets/images/logo-text.png" alt="Logo" class="dark-logo" />
                        </span>
                    </a>
                </div>
                <div class="navbar-collapse">
                    <ul class="navbar-nav mr-auto mt-md-0">
                        <!-- This is  -->
                        <li> <a class="nav-link nav-toggler hidden-md-up text-muted waves-effect waves-dark" href="javascript:void(0)"><i class="mdi mdi-menu"></i></a> </li>
                        <li class="nav-item m-l-10"> <a class="nav-link sidebartoggler hidden-sm-down text-muted waves-effect waves-dark" href="javascript:void(0)"><i class="ti-menu"></i></a> </li>
                    </ul>
                    <ul class="navbar-nav my-lg-0">
                        <li class="nav-item hidden-sm-down search-box">
                            <a class="nav-link hidden-sm-down text-muted waves-effect waves-dark" href="javascript:void(0)"><i class="ti-search"></i></a>
                            <form class="app-search">
                                <input type="text" class="form-control" placeholder="Search & enter">
                                <a class="srh-btn"><i class="ti-close"></i></a>
                            </form>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-muted waves-effect waves-dark" href="#" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><img src="~/Content/VcAssets/images/users/user.png" alt="user" class="profile-pic" /></a>
                            <div class="dropdown-menu dropdown-menu-right scale-up">
                                <ul class="dropdown-user">
                                    <li>
                                        <div class="dw-user-box">
                                            @*<div class="u-img"><img src="~/Content/VcAssets/images/users/user.png" alt="user"></div>*@
                                            <div class="u-text">
                                                <h4>@sm.Name</h4>
                                                <p class="text-muted">@sm.Loginid</p>
                                                @*<a href="pages-profile.html" class="btn btn-rounded btn-danger btn-sm">View Profile</a>*@
                                            </div>

                                        </div>
                                    </li>
                                    <li role="separator" class="divider"></li>
                                    @*<li><a href="#"><i class="ti-user"></i> My Profile</a></li>*@
                                    @*<li><a href="changepassword.html"><i class="ti-settings"></i>Change Password</a></li>*@
                                    <li role="separator" class="divider"></li>
                                    <li><a href="/Home/LogOut"><i class="fa fa-power-off"></i> Logout</a></li>
                                </ul>
                            </div>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <aside class="left-sidebar">
            <!-- Sidebar scroll-->
            <div class="scroll-sidebar">
                <div class="user-profile">

                </div>
                <!-- Sidebar navigation-->
                <nav class="sidebar-nav">
                    <ul id="sidebarnav">
                        <li> <a class="sidebar-link waves-effect waves-dark sidebar-link" href="@Url.Action("Index","Admin")" aria-expanded="false"><i class="mdi mdi-view-dashboard"></i><span class="hide-menu">Dashboard</span></a></li>
                        <li><a href="@Url.Action("GreivanceList","Admin")"><i class="fa fa-user-circle-o"></i><span class="hide-menu">Grievance List </span></a></li>

                    </ul>
                </nav>
            </div>
        </aside>
        <div class="page-wrapper">

            @RenderBody()

            <footer class="footer">
                <div class="row">
                    <div class="col-md-12 text-center">Copyright © @DateTime.Now.Year.ToString(),  <strong>Mahatma Gandhi Kashi Vidyapith</strong> | All Rights Reserved </div>
                </div>
            </footer>
        </div>
    </div>


    <script src="~/Content/js/jquery-confirm.min.js"></script>
    <script src="~/Content/sweetalert/sweetalert.min.js"></script>
    <!-- Bootstrap tether Core JavaScript -->
    <script src="~/Content/VcAssets/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="~/Content/VcAssets/plugins/bootstrap/js/popper.min.js"></script>
    <script src="~/Content/VcAssets/plugins/bootstrap/js/bootstrap.min.js"></script>
    <!-- slimscrollbar scrollbar JavaScript -->
    <script src="~/Content/VcAssets/js/jquery.slimscroll.js"></script>
    <!--Wave Effects -->
    <script src="~/Content/VcAssets/js/waves.js"></script>
    <!--Menu sidebar -->
    <script src="~/Content/VcAssets/js/sidebarmenu.js"></script>
    <!--stickey kit -->
    <script src="~/Content/VcAssets/plugins/sticky-kit-master/dist/sticky-kit.min.js"></script>
    <script src="~/Content/VcAssets/plugins/sparkline/jquery.sparkline.min.js"></script>
    <!--Custom JavaScript -->
    <script src="~/Content/VcAssets/js/custom.min.js"></script>
    <!-- ============================================================== -->
    <!-- This page plugins -->
    <!-- ============================================================== -->
    <!--morris JavaScript -->
    <script src="~/Content/VcAssets/plugins/raphael/raphael-min.js"></script>
    @*<script src="~/Content/VcAssets/plugins/morrisjs/morris.min.js"></script>*@
    <!-- sparkline chart -->
    <script src="~/Content/VcAssets/plugins/sparkline/jquery.sparkline.min.js"></script>
    @*<script src="~/Content/VcAssets/js/dashboard4.js"></script>*@

</body>
</html>

